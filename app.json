{
  "expo": {
    "name": "colour-finder",
    "slug": "colour-finder",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "It is an app to find the name and hex code of color from the picture that was selected from device's local storage or taking a picture using device's camera.",
    "githubUrl": "https://github.com/Iskak83/ColourFinder.git"
  }
}
